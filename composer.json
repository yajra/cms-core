{
  "name": "yajra/cms-core",
  "description": "Core module of YajraCMS.",
  "keywords": [
    "yajracms",
    "laravel",
    "core",
    "module"
  ],
  "homepage": "https://github.com/yajra/cms-core",
  "license": "MIT",
  "authors": [
    {
      "name": "Arjay Angeles",
      "email": "aqangeles@gmail.com",
      "homepage": "http://yajrabox.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "yajra/cms-themes": "^2.7",
    "yajra/laravel-acl": "^3.0",
    "yajra/laravel-auditable": "^2.0",
    "yajra/laravel-datatables": "1.*",
    "yajra/baum": "^1.3",
    "laracasts/flash": "^3.0",
    "laracasts/presenter": "^0.2.1",
    "barryvdh/laravel-snappy": "^0.4",
    "barryvdh/laravel-debugbar": "^3.1.1",
    "davejamesmiller/laravel-breadcrumbs": "^5.0",
    "spatie/laravel-sluggable": "^2.0",
    "spatie/eloquent-sortable": "^3.0",
    "caffeinated/menus": "^3.0",
    "guzzlehttp/guzzle": "^6.1",
    "arrilot/laravel-widgets": "^3.9",
    "intervention/image": "^2.4",
    "rap2hpoutre/laravel-log-viewer": "^1.0",
    "themsaid/laravel-mail-preview": "^2.0",
    "rtconner/laravel-tagging": "^2.2",
    "joshbrw/laravel-module-installer": "^0.1.4",
    "nwidart/laravel-modules": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Yajra\\CMS\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Yajra\\CMS\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "6.0-dev"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
