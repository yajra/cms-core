{
    "name": "yajra/yajracms",
    "description": ":package_description",
    "keywords": [
        "yajra",
        "yajracms"
    ],
    "homepage": "https://github.com/yajra/yajracms",
    "license": "MIT",
    "authors": [
        {
            "name": "Arjay Angeles",
            "email": "aqangeles@gmail.com",
            "homepage": "http://yajrabox.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.9",
        "laravel/framework": "5.2.*",
        "yajra/laravel-oci8": "5.2.*",
        "yajra/laravel-datatables-oracle": "^6.0",
        "yajra/laravel-acl": "^1.0-dev",
        "yajra/laravel-auditable": "^1.0-dev",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "caffeinated/menus": "~2.0",
        "laracasts/flash": "^1.3",
        "spatie/laravel-fractal": "^1.7",
        "barryvdh/laravel-snappy": "0.2.*",
        "guzzlehttp/guzzle": "^6.1",
        "baum/baum": "^1.1.2",
        "laracasts/presenter": "^0.2.1",
        "arrilot/laravel-widgets": "^3.3",
        "intervention/image": "^2.3",
        "spatie/laravel-backup": "^3.7",
        "spatie/laravel-sluggable": "^1.1",
        "rap2hpoutre/laravel-log-viewer": "^0.6.1",
        "themsaid/laravel-mail-preview": "^1.1",
        "barryvdh/laravel-debugbar": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Yajra\\CMS\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Yajra\\CMS\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
